{
    "name": "ng2-screenshot",
    "description": "Angular screenshot in directive for screen capture.",
    "version": "0.0.1",
    "scripts": {
        "link": "ngm link -p src --here",
        "lite-server": "lite-server -c demo/bs-config.json",
        "demo:serve": "run-s build link demo:build lite-server",
        "demo:build": "ng build -prod --aot",
        "test": "karma start",
        "build": "ngm build -p src --clean",
        "minify": "./node_modules/uglify-js/bin/uglifyjs bundles/index.umd.js --screw-ie8 --compress --mangle --comments --output bundles/index.umd.min.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/weihanchen/ng2-screenshot.git"
    },
    "keywords": [
        "angular",
        "angular2",
        "screenshot"
    ],
    "author": "weihanchen",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/weihanchen/angular-screenshot/issues"
    },
    "main": "bundles/ng2-screenshot.umd.js",
    "module": "index.js",
    "typings": "index.d.ts",
    "homepage": "https://github.com/weihanchen/ng2-screenshot",
    "peerDependencies": {
        "@angular/core": "^2.4.7 || ^4.0.0"
    },
    "devDependencies": {
        "@angular/cli": "1.0.0",
        "@angular/common": "^2.4.7 || ^4.0.0",
        "@angular/compiler": "^2.4.7 || ^4.0.0",
        "@angular/compiler-cli": "^2.4.7 || ^4.0.0",
        "@angular/core": "^2.4.7 || ^4.0.0",
        "@angular/forms": "^2.4.7 || ^4.0.0",
        "@angular/platform-browser": "^2.4.7 || ^4.0.0",
        "@angular/platform-browser-dynamic": "^2.4.7 || ^4.0.0",
        "@angular/router": "3.4.3",
        "@types/dom-to-image": "^2.6.0",
        "@types/hammerjs": "^2.0.34",
        "@types/jasmine": "^2.5.51",
        "@types/jquery": "^3.3.31",
        "@types/node": "^7.0.29",
        "awesome-typescript-loader": "^3.1.3",
        "cash-cp": "^0.2.0",
        "classlist-polyfill": "^1.2.0",
        "codelyzer": "^3.0.1",
        "commitizen": "^2.9.6",
        "core-js": "^2.4.1",
        "cz-conventional-changelog": "^2.0.0",
        "dom-to-image": "^2.6.0",
        "html-loader": "^0.5.5",
        "lite-server": "^2.5.4",
        "markdown-loader": "^5.1.0",
        "istanbul-instrumenter-loader": "^2.0.0",
        "jasmine-core": "^2.6.2",
        "jquery": "^3.4.1",
        "karma": "^1.7.0",
        "karma-chrome-launcher": "^2.1.1",
        "karma-coverage": "^1.1.1",
        "karma-firefox-launcher": "^1.0.1",
        "karma-jasmine": "^1.1.0",
        "karma-mocha-reporter": "^2.2.3",
        "karma-remap-coverage": "~0.1.4",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.3",
        "loader-utils": "^1.1.0",
        "ngx-bootstrap": "^2.0.0",
        "ngm-cli": "^1.0.4",
        "npm-run-all": "^4.1.5",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.4.0",
        "semantic-release": "^6.3.6",
        "source-map-loader": "^0.2.1",
